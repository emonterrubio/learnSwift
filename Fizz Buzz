for num in 1...100 {
 if num % 3 == 0 && num % 5 == 0 {
   print ("FizzBuzz")
 } else if num % 3 == 0 {
   print ("Fizz")
 } else if num % 5 == 0 {
   print ("Buzz")
 } else {
   print(num)
 }
}

WHILE LOOP
var counter = 1
while counter <= 100 {
 if counter % 3 == 0 && counter % 5 == 0 {
   print ("FizzBuzz")
 } else if counter % 3 == 0 {
   print ("Fizz")
 } else if counter % 5 == 0 {
   print ("Buzz")
 } else {
   print(counter)
 }
 counter += 1
}
